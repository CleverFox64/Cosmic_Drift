// room script file

function oPortal_Yellow_Interact(Object *theObject, CursorMode mode)
{
  cZeno.Walk(302, 135, eBlock, eWalkableAreas);
  player.Say("WOAH!?");
  player.ChangeRoom(1, 165, 145, eDirectionDown);
  aTransitional_Swipe.Play();
}
function oPortal_Yellow_Look(Object *theObject, CursorMode mode)
{
Display("It's a Yellow Portal! It might take you back from where you came from.");
}

function oPortal_Yellow_Talk(Object *theObject, CursorMode mode)
{
player.Say("Woah, is that a Portal?"); 
}
